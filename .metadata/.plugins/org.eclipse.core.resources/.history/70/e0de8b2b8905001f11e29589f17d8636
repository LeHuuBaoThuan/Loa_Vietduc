typedef enum
{
	MODE_1 = 1,		// music
	MODE_2			// time base
}MODE_STATUS;

void init_handler(ADC_HandleTypeDef* hadc, TIM_HandleTypeDef* htim);
void ON_ALL();
void OFF_ALL();
void Mode1_handl();
void Mode2_handl();
void Handler_Led();

